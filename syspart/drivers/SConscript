Import('syspart_env')

ne2000 = SConscript('ne2000/SConscript')
p3041  = SConscript('p3041/SConscript')
pci    = SConscript('pci/SConscript')
virtio = SConscript('virtio/SConscript')
qemu_vga  = SConscript('qemu-vga/SConscript')
p3041_vga = SConscript('p3041-vga/SConscript')

Return('ne2000', 'p3041', 'pci', 'virtio', 'qemu_vga', 'p3041_vga')

